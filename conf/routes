# Main index.html
GET     /                                           HTML.index
GET     /slim                                       HTML.slim

# Subscribe to real-time updates
GET     /api/subscribe/{token}                      API.subscribe

# Post an eval request to other browsers
POST    /api/request/{token}                        API.postRequest

# Post a response to an eval request
POST    /api/response/{token}                       API.postResponse


# Map static resources from the /app/public folder to the /public path
GET		/public/									staticDir:public
